{
  "name": "algorithm-visualizer-suite",
  "version": "1.0.0",
  "description": "A beautiful algorithm visualization suite made with Electron and React.",
  "main": "out/main/index.js",
  "author": "Antigravity",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "release": "electron-builder --publish always",
    "pack": "electron-builder --dir"
  },
  "build": {
    "appId": "com.algorithm-visualizer.app",
    "productName": "Algorithm Visualizer Suite",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "out/**/*",
      "package.json",
      "!venv",
      "!venv_13",
      "!python_legacy"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "swastikparmar987",
        "repo": "Algorithm-Visualizer-Suite"
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.education"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ]
    }
  },
  "dependencies": {
    "@google/genai": "^1.42.0",
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^9.109.2",
    "@react-three/fiber": "^8.17.6",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.3.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.8.3",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.344.0",
    "mongoose": "^9.2.1",
    "nodemailer": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.7.0",
    "three": "^0.183.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^29.4.6",
    "electron-builder": "^26.8.1",
    "electron-vite": "^2.3.0",
    "vite": "^5.4.21"
  }
}